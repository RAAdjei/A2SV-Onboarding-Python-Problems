class Solution:
    def replaceElements(self, arr: List[int]) -> List[int]:

        n = len(arr)
        max_num = -1

        for i in range(n-1, -1, -1):
             temp = arr[i]
             arr[i] = max_num
             max_num = max(max_num, temp)

        return arr

"""
Time - O(N)
Space - O(N)
"""
