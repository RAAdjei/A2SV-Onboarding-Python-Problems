t = int(input())
 
for _ in range(t):
    n = int(input())
    arr = list(map(int, input().split()))
    max_v = max(arr)
    min_v = min(arr)
    
    for i in range(n):
        if arr[i] == max_v:
            max_i = i +1
            
        if arr[i] == min_v:
            min_i = i +1
    print(min_i, max_i)

"""
Time - O(N)
Space - O(1)
"""
